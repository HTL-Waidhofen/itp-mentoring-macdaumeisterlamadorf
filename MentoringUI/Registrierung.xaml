<Page x:Class="MentoringUI.Registrierung"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MentoringUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Registrierung">

    <DockPanel LastChildFill="False">
        <Grid DockPanel.Dock="Top">
            <Label FontWeight="Bold" FontSize="40" HorizontalAlignment="Center">Registrieren</Label>
            <Image x:Name="Logo_img" Source="HTL-Logo.png" Height="100" HorizontalAlignment="Right" ></Image>
        </Grid>
    </DockPanel>
</Page>
